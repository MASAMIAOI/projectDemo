<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.masamiaoi.sqlite.dao.ProMapper">    <!--  查询集合     -->    <select id="list" resultType="com.masamiaoi.sqlite.po.ProPO">        SELECT *        FROM pro    </select>    <!--  条件查询  -->    <select id="query" parameterType="com.masamiaoi.sqlite.po.ProPO"            resultType="com.masamiaoi.sqlite.po.ProPO">        SELECT * FROM pro        WHERE 1=1        <if test="databaseKey!=null and databaseKey!=''">            AND database_key like CONCAT(CONCAT('%',#{databaseKey}),'%')        </if>    </select>    <!--    添加  -->    <insert id="add" parameterType="com.masamiaoi.sqlite.po.ProPO">        INSERT INTO pro(id, databaseKey, databaseUrl, createTime)        VALUES (#{id}, #{databaseKey}, #{databaseUrl}, #{createTime})    </insert>    <!--    删除全部    -->    <delete id="delAll">        DELETE        FROM pro    </delete>    <!--    根据 id 删除-->    <delete id="del" parameterType="java.lang.Integer">        DELETE        FROM pro        WHERE id = #{id}    </delete></mapper>